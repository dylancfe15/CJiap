  pod 'FBSDKLoginKit'
  pod 'Firebase/Auth'
  pod 'Firebase/Database'
  pod 'Firebase/Core'
  
  target 'CJiapTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'CJiapUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
